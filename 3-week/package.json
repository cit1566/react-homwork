{
  "private": true,
  "type": "module",
  "name": "3-week-clon",
  "description": "리액트 빌드 환경을 구성합니다.",
  "version": "1.0.0",
  "scripts": {
    "dev": "bun index.html",
    "prod": "run-s clean build",
    "build": "bun run .bun/build.js",
    "preview": "bun run .bun/preview.js",
    "format:check": "prettier --check --cache \"src/**/*.{js,jsx,json}\"",
    "format:fix": "prettier --write --cache --list-different \"src/**/*.{js,jsx,json}\"",
    "lint:check": "eslint \"src/**/*.{js,jsx,json}\" --cache --cache-location ./node_modules/.cache/eslint/.eslintcache",
    "lint:fix": "bun lint:check --fix",
    "clean": "rimraf -rf dist"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "eslint-config-prettier": "^10.1.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "eslint": "^9.33.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2"
  }
}
